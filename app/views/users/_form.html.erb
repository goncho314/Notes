<%= form_with(model: user, local: true) do |form| %>
  

  <div class="field">
    <%= label_tag :username %>
    <%= text_field_tag :username, params[:username] %>
  </div>

  <div class="field">
    <%= form.label :password %>
    <%= password_field_tag :password, params[:password] %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
